    {
      "builds": [
        {
          "src": "frontend/my-app/package.json",
          "use": "@vercel/static-build",
          "config": {
            "distDir": "frontend/build" 
          }
        },
        {
          "src": "backend/app.py",
          "use": "@vercel/python"
        }
      ],
      "routes": [
        {
          "src": "/(.*)",
          "dest": "/backend/app.py"
        },
        {
          "src": "/(.*)",
          "dest": "/frontend/my-app/$1"
        }
      ],
      
    "rewrites": [
    {
      "source": "/welcome",
      "destination": "/section/welcome.html"
    }
  ]
}
    